<!--平台服务器配置-->
<root>
	<!-- 服务器基本信息 sync_log-日志是否异步输出(0表示同步1表示异步) work_path-工作目录-->
	<server_info sync_log = "0" work_path = "F:/designer/resource/server/config"/>
	<!--监听gcc服务器的地址-->
	<listen_gcc ip = "0.0.0.0" port = "10000"/>
	<!--监听web的地址-->
	<listen_web ip = "0.0.0.0" port = "10010"/>
	<!--global数据库配置-->
	<global_db ip = "127.0.0.1" port = "3306" user = "root" password = "123456" name = "global_db" charset = "utf8"/>
</root>